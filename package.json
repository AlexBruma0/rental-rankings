{
  "name": "rental-app",
  "version": "1.0.0",
  "description": "Rental App",
  "workspaces": [
    "client",
    "api"
  ],
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "prettier": "2.8.2"
  },
  "scripts": {
    "client": "cd client && npm run dev",
    "install:client": "cd client && npm ci --ignore-scripts",
    "build": "cd client && npm run build",
    "lint:client": "cd client && npx prettier 'src/**/*.{js,ts,jsx,tsx,json}' --check --ignore-path .gitignore",
    "format:client": "cd client && npx prettier 'src/**/*.{js,ts,jsx,tsx,json}' --write --ignore-path .gitignore",
    "postinstall": "npm run install:client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RentalReviews/Rental-App.git"
  },
  "bugs": {
    "url": "https://github.com/RentalReviews/Rental-App/issues"
  },
  "homepage": "https://github.com/RentalReviews/Rental-App"
}
